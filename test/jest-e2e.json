{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": "\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@cache/cache-manager/(.*)": "<rootDir>/../libs/cache-manager/src/$1",
    "@cache/cache-manager": "<rootDir>/../libs/cache-manager/src"
  }
}